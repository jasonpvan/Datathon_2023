#include<iostream>
#include<fstream>
#include<vector>
using namespace std;

int main(){

  string Comodin;
  ifstream Data("PRODUCTO_DATA_BUENO.txt");
  vector<vector<string> > mat(9222, vector<string>(7));
    for(int i=0; i<9222; ++i){
        for (int g = 0; g < 7; ++g){
                Data>>Comodin;
                mat[i][g]=Comodin;
                cout<< mat[i][g]<<" ";
            }
            cout<<endl;
        }
    }
